def E_Stop(PID):
    # Check pin status until pressed
    while (GPIO.input(Emergency_Pin) & GPIO.input(Run_Switch)): 
        # Wait 10 ms before checking again
        time.sleep(.01)
    # Pause or kill the other program
    if (GPIO.input(Run_Switch)):
        #pause somehow, wait for switch, and restart. Then self call
        a = 1
    else:
        subprocess.run(["/home/dorna/Python/killfile.sh", str(PID)], check=True)
        # Kill the PID of other program

numList = [4, 17, 27, 22, 5, 6, 13, 19, 26, 23, 24, 12, 16, 20, 21, 2, 3, 10, 9, 11, 8, 7, 1]
def get_GPIO():
    for i in numList:
        GPIO.setup(i, GPIO.IN)
        print("Pin Number: {}, Output: {}".format(str(i), str(GPIO.input(i))))
