def accguy(revs):
    counter = 0
    robot = ROBOT
    ready_cart =  [-0.786, 300.067, 268.442, -21.994, 0.0, 29.43, 0.0, 0.0]

    zero = homing_position(ROBOT)
    
    for i in range(2 * revs - 1):
        robot.play(TIMEOUT = -1, 
        cmd = "jmove",
        rel=0, 
        x=ready_cart[0], 
        y=ready_cart[1], 
        z=ready_cart[2], 
        a=ready_cart[3], 
        b=ready_cart[4],
        c=ready_cart[5] - zero,
        vel=15, 
        accel=2000, 
        jerk=4000
        )
    
        print("Number: {}".format(str(counter)))
        print("")
        wash_single_screen(zero, counter)
        wash_single_perimeter(zero, counter)
        if counter == 0:
            counter = 1
        else:
            counter = 0
    robot.play(TIMEOUT = -1, 
        cmd = "jmove",
        rel=0, 
        x=ready_cart[0], 
        y=ready_cart[1], 
        z=ready_cart[2], 
        a=ready_cart[3], 
        b=ready_cart[4],
        c=ready_cart[5] - zero,
        vel=15, 
        accel=2000, 
        jerk=4000
        )



    

    wash_single_screen(zero, 1)
    wash_single_perimeter(zero, 1)
def main():
    pid = os.getpid()
    try:
        print("Connected") if ROBOT.connect() else sys.exit()
        #help()
        while True:
            command = input("> ")
            if command in ['q', 'quit']:
                break
            elif command in ['m', 'motor']:
                motor_switch(ROBOT)
            elif command in ['g', 'get']:
                get_all_joint_positions()
            elif command in ['h', 'homing']:
                homing_position(ROBOT)
            elif command in ['s', 'start']:
                start_position(ROBOT, 10)
            elif command in ['L', 'Lump']:
                pump_activate(ROBOT)
            elif command in ['t', 'test']:
                accguy(2)
                
            elif command in ['c','clear']:
                clear_alarm(ROBOT)
            else:
                help()
    except Exception as e:
        print(e)
    finally:
        motor_switch(ROBOT)
        print("Disconnected")
        ROBOT.close(ROBOT)






if __name__ == "__main__":
    print(f"Version: {VERSION}")
    main()
